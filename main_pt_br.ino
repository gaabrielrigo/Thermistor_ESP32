void ReadSensor() {
  if (simulating) {
    Temperature = 20.2 + random(-15, 15) / 10.0; // Generate a random temperature value between 18.7° and 21.7°
    Humidity    = random(45, 55);                // Generate a random humidity value between 45% and 55%
  }
  
    else{
    bool  isOn = NULL;
    double Beta = 3308.11;  // Beta value
    double Ro = 10000.0;   // Resistance of Thermistor at 25 degree Celsius
    double R1 = 10000.0;   // voltage divider resistor value
    double adcMax = 4095.0;
    double Vs = 3.3;
    double Vout, Rt = 0;
    double T, Tc, Tf = 0;
    double adc = 0;
    double To = 298.15;    // Temperature in Kelvin for 25 degree Celsius
    
    double val_adc[5];

    for(int i=0;i<5;i++){
    val_adc[i] = analogRead(35);
    adc += val_adc[i];
    } 
    adc = adc/5.0;
    adc = ADC_LUT[(int)adc];

    Vout = adc * Vs/adcMax;
    Rt = R1 * Vout / (Vs - Vout);
    
    T = 1/(1/To + log(Rt/Ro)/Beta);    // Temperature in Kelvin
    Temperature = (int)(T - 273.15);                   // Celsius
    //Humidity    = 45; 
    //Tf = Tc * 9 / 5 + 32;              // Fahrenheit

  }
}
